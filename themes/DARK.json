  "tokenColors": [
    {
      "scope": [
        "comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
